<h1 align="center">Rate My Rental Agent Users</h1>
<div align="center">
  <%= will_paginate renderer: BootstrapPagination::Rails %>
  <% @users.each do |user| %>
      <ul class="listing">
        <div class="row">
          <div class="well col-md-4 col-md-offset-4">
            <li class="title">
              <%= link_to user.username, user_path(user) %>
            </li>
            <li>
              <small><%= pluralize(user.ratings.count, "rating") if user.ratings %> </small>
            </li>
          </div>
        </div>
      </ul>
  <% end %>
  <%= will_paginate renderer: BootstrapPagination::Rails %>
</div>